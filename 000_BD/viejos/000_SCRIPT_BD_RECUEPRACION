CREATE TABLE IF NOT EXISTS `bd_sis_inventario_tmp`.`tab_personas` (
  `Id_Per` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
  `Cedula_Per` VARCHAR(16) NOT NULL,
  `Tipo_Identificacion_Per` CHAR(1) NULL DEFAULT NULL COMMENT '1=cedula\n2=Residencia\n3=Pasaporte',
  `Id_Pai_Per` SMALLINT(5) UNSIGNED NULL DEFAULT NULL,
  `Id_Pro_Per` MEDIUMINT(8) UNSIGNED NULL DEFAULT NULL,
  `Id_Can_Per` MEDIUMINT(8) UNSIGNED NULL DEFAULT NULL,
  `Id_Dist_Per` MEDIUMINT(8) UNSIGNED NULL DEFAULT NULL,
  `Id_GA_Per` TINYINT(3) UNSIGNED NULL DEFAULT NULL,
  `Nombre_Per` VARCHAR(60) NULL DEFAULT NULL COMMENT 'Nombre de la persona',
  `Apellido1_Per` VARCHAR(50) NULL DEFAULT NULL COMMENT 'Primer Apellido',
  `Apellido2_Per` VARCHAR(50) NULL DEFAULT NULL COMMENT 'Segundo apellido',
  `Genero_Per` CHAR(1) NULL DEFAULT NULL COMMENT '1=Masculino\n2=Femenino',
  `Direccion_Per` BLOB NULL DEFAULT NULL COMMENT 'Direccion fisica de la persona',
  `Fecha_Nacimiento_Per` DATE NULL DEFAULT NULL COMMENT 'Fecha de Nacimiento de la persona\n',
  `Foto_Per` VARCHAR(45) NULL DEFAULT NULL COMMENT 'Foto de la persona',
  `Activo_Per` CHAR(1) NULL DEFAULT NULL COMMENT '1=activo\n0=inactivo',
  PRIMARY KEY (`Id_Per`),
  INDEX `fk_tab_personas_tab_paises1_idx` (`Id_Pai_Per` ASC),
  INDEX `fk_tab_personas_tab_provincias1_idx` (`Id_Pro_Per` ASC),
  INDEX `fk_tab_personas_tab_cantones1_idx` (`Id_Can_Per` ASC),
  INDEX `fk_tab_personas_tab_distritos1_idx` (`Id_Dist_Per` ASC),
  INDEX `fk_tab_personas_tab_grados_academicos1_idx` (`Id_GA_Per` ASC),
  CONSTRAINT `fk_tab_personas_tab_cantones1`
    FOREIGN KEY (`Id_Can_Per`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_cantones` (`Id_Can`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_personas_tab_distritos1`
    FOREIGN KEY (`Id_Dist_Per`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_distritos` (`Id_Dist`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_personas_tab_grados_academicos1`
    FOREIGN KEY (`Id_GA_Per`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_grados_academicos` (`Id_GA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_personas_tab_paises1`
    FOREIGN KEY (`Id_Pai_Per`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_paises` (`Id_Pai`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_personas_tab_provincias1`
    FOREIGN KEY (`Id_Pro_Per`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_provincias` (`Id_Pro`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
