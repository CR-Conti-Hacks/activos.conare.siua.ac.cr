  `Id_Preg_Usu` TINYINT(3) UNSIGNED NOT NULL COMMENT 'Id de pregunta para recuperacion de contraseÃ±a',
  `Password_Usu` VARCHAR(100) NULL DEFAULT NULL COMMENT 'ContraseÃ±a de usuario',
  `Estado_Conexion_Usu` TINYINT(1) NULL DEFAULT NULL COMMENT '1=conectado\n0=Inactivo',
