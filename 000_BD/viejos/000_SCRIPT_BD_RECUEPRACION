  `Activo_SOL` CHAR(1) NULL DEFAULT NULL,
  PRIMARY KEY (`Id_SOL`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = latin1;

CREATE TABLE IF NOT EXISTS `bd_sis_inventario_tmp`.`tab_preslla_prestamos` (
  `Id_PRES` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
  `Id_LLA_PRESS` INT(10) UNSIGNED NOT NULL,
  `Id_SOL_PRESS` INT(10) UNSIGNED NOT NULL,
  `Id_Per_Usu_Entrega_PRES` INT(10) UNSIGNED NOT NULL,
  `Id_Per_Usu_Recibe_PRES` INT(10) UNSIGNED NULL DEFAULT NULL,
  `Fecha_Hora_Entrega_PRES` DATETIME NULL DEFAULT NULL,
  `Fecha_Hora_Recepcion_PRES` DATETIME NULL DEFAULT NULL,
  PRIMARY KEY (`Id_PRES`),
  INDEX `fk_tab_preslla_prestamos_tab_usuarios1_idx` (`Id_Per_Usu_Entrega_PRES` ASC),
  INDEX `fk_tab_preslla_prestamos_tab_usuarios2_idx` (`Id_Per_Usu_Recibe_PRES` ASC),
  INDEX `fk_tab_preslla_prestamos_tab_preslla_llaves1_idx` (`Id_LLA_PRESS` ASC),
  INDEX `fk_tab_preslla_prestamos_tab_preslla_solicitantes1_idx` (`Id_SOL_PRESS` ASC),
  CONSTRAINT `fk_tab_preslla_prestamos_tab_usuarios1`
    FOREIGN KEY (`Id_Per_Usu_Entrega_PRES`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_usuarios` (`Id_Per_Usu`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_preslla_prestamos_tab_usuarios2`
    FOREIGN KEY (`Id_Per_Usu_Recibe_PRES`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_usuarios` (`Id_Per_Usu`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_preslla_prestamos_tab_preslla_llaves1`
    FOREIGN KEY (`Id_LLA_PRESS`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_preslla_llaves` (`Id_LLA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_preslla_prestamos_tab_preslla_solicitantes1`
    FOREIGN KEY (`Id_SOL_PRESS`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_preslla_solicitantes` (`Id_SOL`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = latin1;


