  INDEX `fk_tab_usuarios_tab_perfiles1_idx` (`Id_Rol_Usu` ASC),
  INDEX `fk_tab_usuarios_tab_preguntas1_idx` (`Id_Preg_Usu` ASC),
  INDEX `fk_tab_usuarios_tab_personas1_idx` (`Id_Per_Usu` ASC),
  CONSTRAINT `fk_tab_usuarios_tab_perfiles1`
    FOREIGN KEY (`Id_Rol_Usu`)
    REFERENCES `bd_sis_inventario_tmp`.`tab_roles` (`Id_Rol`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_tab_usuarios_tab_personas1`
